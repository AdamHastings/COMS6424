#ifndef NAND_DRIVER_H   /* prevent circular inclusions */
#define NAND_DRIVER_H   /* by using protection macros */

#include "xil_types.h"

/*
 * Initialize the nand flash controller (nfc) by setting and clearing the reset bit
 */
void nfc_init();

void nfc_write();

uint32_t nfc_read();

void nfc_erase();


#endif
